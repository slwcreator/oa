<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.slwer.oa.mapper.LeaveFormMapper">
    <insert id="insert" parameterType="com.slwer.oa.entity.LeaveForm"
            useGeneratedKeys="true" keyProperty="formId" keyColumn="form_id">
        INSERT INTO `oa`.`adm_leave_form` (`employee_id`, `form_type`, `start_time`, `end_time`, `reason`,
        `create_time`, `state`)
        VALUES (#{employeeId}, #{formType}, #{startTime}, #{endTime}, #{reason}, #{createTime}, #{state});
    </insert>
</mapper>